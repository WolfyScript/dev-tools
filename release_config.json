{
  "categories": [
    {
      "title": "## ‚ú® Next Generation Features & Changes",
      "labels": [
        "epoch-change"
      ]
    },
    {
      "title": "## üöß Breaking Changes",
      "labels": [
        "breaking-change"
      ]
    },
    {
      "title": "## üöÄ Features",
      "labels": [
        "enhancement",
        "feat"
      ]
    },
    {
      "title": "## üêõ Fixes",
      "labels": [
        "fix",
        "bug"
      ]
    },
    {
      "title": "## üß™ Tests",
      "labels": [
        "test"
      ]
    },
    {
      "title": "## üì¶ Misc",
      "labels": []
    }
  ],
  "ignore_labels": [
    "skip-changelog"
  ],
  "label_extractor": [
    {
      "pattern": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test){1}(\\([\\w\\-\\.]+\\))?(!)?: ([\\w ])+([\\s\\S]*)",
      "target": "$1"
    },
    {
      "pattern": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test)(\\([\\w\\-\\.]+\\)){1}?(!)?: ([\\w ])+([\\s\\S]*)",
      "target": "scope-$1"
    },
    {
      "pattern": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test)(\\([\\w\\-\\.]+\\))?!: ([\\w ])+([\\s\\S]*)",
      "target": "breaking-change"
    },
    {
      "pattern": "^BREAKING CHANGE:(.*)",
      "target": "breaking-change"
    },
    {
      "pattern": "^ignore(\\([\\w\\-\\.]+\\))?: ([\\w ])+([\\s\\S]*)",
      "target": "skip-changelog"
    }
  ],
  "tag_resolver": {
    "method": "sort"
  },
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "#{{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n#{{UNCATEGORIZED}}\n</details>",
  "pr_template": "- #{{TITLE}} (##{{NUMBER}}) - #{{AUTHOR}}",
  "commit_template": "- #{{TITLE}} ([`#{{SHORT_MERGE_SHA}}`](https://github.com/${{ github.repository }}/commit/#{{MERGE_SHA}}))",
  "custom_placeholders": [
    {
      "name": "SHORT_MERGE_SHA",
      "source": "MERGE_SHA",
      "transformer": {
        "pattern": "(.*){7}(.*)?",
        "target": "- $1"
      }
    }
  ]
  "empty_template": "- no changes"
}